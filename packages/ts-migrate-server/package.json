    },
    {
      "name": "Joe Lencioni",
      "url": "https://github.com/lencioni"
    }
  ],
  "homepage": "https://github.com/airbnb/ts-migrate#readme",
  "license": "MIT",
  "dependencies": {
    "@ts-morph/bootstrap": "^0.16.0",
    "pretty-ms": "^7.0.1",
    "updatable-log": "^0.2.0"
  },
  "gitHead": "7acf6067f15c9bb367cda9c47fcfb4203dcc54f3",
  "peerDependencies": {
    "typescript": ">4.0"
  }
