{
  "name": "ts-migrate",
  "version": "1.0.0",
  "description": "Migrate JS projects to compiling TS projects.",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "ts-migrate": "build/cli.js",
    "ts-migrate-full": "bin/ts-migrate-full.sh"
  },
  "scripts": {
    "test": "jest --config jest-config.json",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx --ignore-path ../../.eslintignore",
    "build": "tsc -p tsconfig.json",
    "prepublish": "yarn build",
    "clean": "rm -rf build"
  },
  "contributors": [
    {
      "name": "Brie Bunge",
      "url": "https://github.com/brieb"
    },
    {
      "name": "Sergii Rudenko",
      "url": "https://github.com/Rudeg"
    },
    {
      "name": "Elliot Sachs",
      "url": "https://github.com/elliotsa"
    },
    {
      "name": "John Haytko",
      "url": "https://github.com/jjjjhhhhhh"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airbnb/ts-migrate.git"
  },
  "bugs": {
    "url": "https://github.com/airbnb/ts-migrate/issues"
  },
  "homepage": "https://github.com/airbnb/ts-migrate#readme",
  "license": "MIT",
  "dependencies": {
    "ts-migrate-server": "^1.0.0",
    "ts-migrate-plugins": "^1.0.0",
    "create-jest-runner": "^0.5.3",
    "eslint": "^6.1.0",
    "json5": "^2.1.1",
    "json5-writer": "^0.1.8",
    "typescript": "^3.8.2",
    "updatable-log": "^0.2.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/json5": "0.0.30",
    "@types/react": "^16.9.16",
    "@types/rimraf": "^2.0.3",
    "@types/yargs": "^13.0.3",
    "glob": "^7.1.6",
    "react": "^16.12.0",
    "rimraf": "^3.0.0"
  }
}