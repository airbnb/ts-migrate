      "url": "https://github.com/elliotsa"
    },
    {
      "name": "John Haytko",
      "url": "https://github.com/jjjjhhhhhh"
    },
    {
      "name": "Joe Lencioni",
      "url": "https://github.com/lencioni"
    }
  ],
  "homepage": "https://github.com/airbnb/ts-migrate#readme",
  "license": "MIT",
  "dependencies": {
    "create-jest-runner": "^0.5.3",
    "json5": "^2.1.1",
    "json5-writer": "^0.1.8",
    "ts-migrate-plugins": "^0.1.0",
    "ts-migrate-server": "^0.1.0",
    "typescript": "3.9.7",
    "updatable-log": "^0.2.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.16",
    "glob": "^7.1.6",
